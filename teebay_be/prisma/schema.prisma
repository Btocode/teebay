generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // Set your PostgreSQL connection URL here
}

model User {
  id        Int      @id @default(autoincrement())
  firstname String
  lastname  String
  password  String
  email     String   @unique
  address   String
  phone     String
}
